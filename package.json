{
  "name": "@formkit/auto-animate",
  "version": "1.0.0-beta.6",
  "description": "Add motion to your apps with a single line of code.",
  "private": true,
  "keywords": [
    "animation",
    "transition",
    "react",
    "vue",
    "angular",
    "svelte",
    "solid"
  ],
  "type": "module",
  "main": "index.mjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "repository": "https://github.com/formkit/auto-animate.git",
  "author": "Justin Schroeder <justin@formkit.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cd docs && vite --config=./vite.config.ts",
    "build": "node ./.build.mjs",
    "build-docs": "cd docs && vite-ssg build --config=./docs/vite.config.ts",
    "publish": "node ./.build.mjs --publish",
    "cypress:open": "cypress open",
    "cypress:server": "cd docs && npx vite --config=./vite.config.ts --port=5555"
  },
  "exports": {
    "./vue": {
      "import": "./vue/index.mjs",
      "types": "./vue/index.d.ts",
      "default": "./vue/index.mjs"
    },
    "./react": {
      "import": "./react/index.mjs",
      "types": "./react/index.d.ts",
      "default": "./react/index.mjs"
    },
    "./solid": {
      "import": "./solid/index.mjs",
      "types": "./solid/index.d.ts",
      "default": "./solid/index.mjs"
    },
    "./angular": {
      "import": "./angular/index.mjs",
      "types": "./angular/index.d.ts",
      "default": "./angular/index.mjs"
    },
    ".": {
      "import": "./index.mjs",
      "types": "./index.d.ts",
      "default": "./index.mjs"
    }
  },
  "devDependencies": {
    "@angular/core": "^13.3.11",
    "@formkit/vue": "^1.0.0-beta.8",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/node": "^17.0.33",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.9",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vueuse/head": "^0.7.6",
    "brotli-size": "^4.0.0",
    "chalk": "^5.0.1",
    "cypress": "^9.6.1",
    "execa": "^6.1.0",
    "pretty-bytes": "^6.0.0",
    "prompts": "^2.4.2",
    "react": "^18.1.0",
    "rollup": "^2.72.1",
    "rollup-plugin-terser": "^7.0.2",
    "shx": "^0.3.4",
    "solid-js": "^1.6.8",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-ssg": "^0.20.0",
    "vue": "^3.2.33",
    "vue-github-button": "^3.0.3",
    "vue-router": "4"
  },
  "dependencies": {}
}
